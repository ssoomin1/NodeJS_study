<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table border="1">
        <tr><td>순번</td><td>파일명</td><td>비고</td>
    <% rows.forEach(function(list){ %>
        <tr><td><%= list.id %></td><td><a href="/down/<%=list.file_name %>"><%= list.file_name %></a></td><td><a href="/update/<%=list.id %>">수정</a>&nbsp;<a href="/delete/<%=list.id %>">삭제</a></td></tr>
   <% })%>
    </table>
</body>
</html>